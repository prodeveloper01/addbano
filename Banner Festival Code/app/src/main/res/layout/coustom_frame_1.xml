<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/ivframebg"
        android:layout_width="match_parent"
        android:layout_height="@dimen/square_size"
        android:background="@drawable/frame_69"
        android:orientation="vertical" />

    <FrameLayout
        android:id="@+id/frameLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_3sdp">

            <ImageView
                android:id="@+id/iv_logolayout2"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_gravity="start"
                android:layout_margin="@dimen/_3sdp"
                android:scaleType="fitStart" />
        </LinearLayout>


    </FrameLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:gravity="bottom"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="7dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ll_f2_email"

                android:layout_width="match_parent"
                android:layout_height="@dimen/_15sdp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <View
                    android:layout_width="@dimen/_8sdp"
                    android:layout_height="@dimen/_2sdp"
                    android:layout_gravity="center_vertical"
                    android:background="@color/colorBlack"
                    android:visibility="gone" />

                <LinearLayout
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="15dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_emaillayout2"
                        android:layout_width="@dimen/_10sdp"
                        android:layout_height="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_6sdp"
                        android:src="@drawable/ic_baseline_email_24" />


                    <TextView
                        android:id="@+id/tv_emaillayout2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="your company email@gmail.com"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_6sdp" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_f2_call"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_15sdp"
                android:layout_weight="1"
                android:orientation="horizontal">


                <LinearLayout
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">


                    <ImageView
                        android:id="@+id/iv_calllayout2"
                        android:layout_width="@dimen/_10sdp"
                        android:layout_height="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_6sdp"
                        android:src="@drawable/ic_baseline_call_24" />


                    <TextView
                        android:id="@+id/tv_mobilenumberlayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:text="+91 12345 67895"
                        android:textColor="@color/black"
                        android:textSize="7sp" />


                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_f2_web"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_17sdp"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/_8sdp"
                android:layout_height="@dimen/_2sdp"
                android:layout_gravity="center_vertical"
                android:background="@color/colorBlack"
                android:visibility="gone" />


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/iv_weblayout2"
                    android:layout_width="@dimen/_10sdp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_6sdp"
                    android:src="@drawable/ic_web" />

                <TextView
                    android:id="@+id/tv_websitelayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:text="www.your website.com"
                    android:textColor="@color/colorWhite"
                    android:textSize="7sp" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</FrameLayout>
