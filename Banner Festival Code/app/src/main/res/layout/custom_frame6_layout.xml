<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <ImageView
            android:id="@+id/iv_logolayout6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:scaleType="fitXY"
            android:layout_alignParentLeft="true"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp">
        </ImageView>
        <LinearLayout
            android:layout_width="140dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:id="@+id/ll_f6_call"
            android:background="@drawable/f10_bgimage1"
            android:backgroundTint="@color/color17a"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_calllayout6"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_marginLeft="4dp"
                android:src="@drawable/ic_baseline_call_24"
                android:layout_marginRight="4dp"
                android:tint="@color/white">
            </ImageView>
            <TextView
                android:id="@+id/tv_mobilenumberlayout6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+91 987654321"
                android:textColor="@color/white"
                android:textSize="8dp"></TextView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"

            android:layout_marginRight="2dp"
            android:layout_marginLeft="2dp"
            android:gravity="center"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"

                android:background="@drawable/f10_bgimage2"
                android:backgroundTint="@color/color18">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:id="@+id/ll_f6_web"
                    android:layout_marginLeft="5dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true">
                    <ImageView
                        android:id="@+id/iv_weblayout6"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@drawable/ic_web"
                        android:tint="@color/white"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="4dp"></ImageView>
                    <TextView
                        android:id="@+id/tv_websitelayout6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="www.companywebsite.com"
                        android:textColor="@color/white"
                        android:textSize="7dp"
                        ></TextView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/ll_f6_email"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <ImageView
                            android:id="@+id/iv_emaillayout6"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:src="@drawable/ic_baseline_email_24"
                            android:tint="@color/white"
                            android:layout_marginRight="4dp">
                        </ImageView>
                        <TextView
                            android:id="@+id/tv_emaillayout6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="companyemail@gmail.com"
                            android:textSize="8dp"
                            android:layout_marginRight="8dp"
                            android:textColor="@color/white"
                            ></TextView>
                    </LinearLayout>
                </LinearLayout>

            </RelativeLayout>
           <!-- <ImageView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"
                android:src="@color/colorBlack">

            </ImageView>-->


          <!--  <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/f_location"
                    android:layout_marginRight="4dp">
                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Company Address"
                    android:textColor="@color/colorBlack"
                    android:textSize="15dp"></TextView>
            </LinearLayout>-->



        </LinearLayout>


    </RelativeLayout>
</RelativeLayout>