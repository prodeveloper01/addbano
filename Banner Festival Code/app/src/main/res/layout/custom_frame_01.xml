<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLogo01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_margin="@dimen/_5sdp">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageView
                android:id="@+id/iv_logolayout_01"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/framePhone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_vertical"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_19sdp">

        <LinearLayout
            android:id="@+id/ll_f01_call"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:paddingRight="10dp"
            android:background="@drawable/frame_bg"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_calllayout_01"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:background="@color/black"
                android:src="@drawable/ic_baseline_call_24"
                app:tint="@color/white" />

            <TextView
                android:id="@+id/tv_mobilenumberlayout_01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:paddingLeft="@dimen/_5sdp"
                android:textColor="@color/black"
                android:textSize="@dimen/_8sdp"
                android:textStyle="bold" />
        </LinearLayout>
    </FrameLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_vertical"
        android:paddingRight="7dp"
        android:layout_marginBottom="@dimen/_3sdp"
        android:orientation="horizontal"
        android:background="@drawable/frame_bg"
        android:weightSum="2">


        <LinearLayout
            android:id="@+id/ll_f01_email"
            android:layout_width="@dimen/_130sdp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_emaillayout_01"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:src="@drawable/ic_email"
                android:background="@color/black"/>


            <TextView
                android:id="@+id/tv_emaillayout_01"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="your company email@gmail.com"
                android:textColor="@color/black"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textSize="8dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_f01_web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/iv_weblayout_01"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:src="@drawable/ic_web"
                android:background="@color/black"
                app:tint="@color/white" />

            <TextView
                android:id="@+id/tv_websitelayout_01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_gravity="center"
                android:text="www.your website.com"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="8dp" />

        </LinearLayout>
    </LinearLayout>

</FrameLayout>
