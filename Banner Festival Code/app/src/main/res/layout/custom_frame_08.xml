<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/ivframebg"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_gravity="bottom"
        android:src="@drawable/frame_008" />


    <FrameLayout
        android:id="@+id/frameLogo08"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_margin="@dimen/_5sdp">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginLeft="2dp">

            <ImageView
                android:id="@+id/iv_logolayout_08"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp" />
        </LinearLayout>

    </FrameLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


       <!-- <LinearLayout
            android:id="@+id/ll_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_gravity="bottom|center_vertical"
            android:layout_marginLeft="@dimen/_5sdp"
            android:orientation="vertical"
            android:paddingRight="7dp">






            </LinearLayout>

        </LinearLayout>-->

        <LinearLayout
            android:id="@+id/ll_f08_call"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_above="@id/ll_f08_location"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginBottom="5dp"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingRight="10dp">

            <ImageView
                android:id="@+id/iv_calllayout_08"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:src="@drawable/whatsapp" />

            <TextView
                android:id="@+id/tv_mobilenumberlayout_08"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:paddingLeft="@dimen/_5sdp"
                android:textColor="@color/white"
                android:textSize="@dimen/_7sdp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_f08_email"
            android:layout_width="@dimen/_110sdp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_toRightOf="@id/ll_f08_call"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="5dp"
            android:layout_above="@id/ll_f08_location"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_emaillayout_08"
                android:layout_width="@dimen/_10sdp"
                android:layout_height="@dimen/_10sdp"
                android:src="@drawable/email_f08" />


            <TextView
                android:id="@+id/tv_emaillayout_08"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="your company email@gmail.com"
                android:textColor="@color/white"
                android:textSize="@dimen/_6sdp"
                android:textStyle="bold" />

        </LinearLayout>

        <View
            android:id="@+id/view_line"
            android:layout_width="1dp"
            android:layout_height="15dp"
            android:layout_margin="4dp"
            android:layout_toRightOf="@id/ll_f08_email"
            android:layout_above="@id/ll_f08_location"
            android:background="@color/white"/>

        <LinearLayout
            android:id="@+id/ll_f08_web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginBottom="5dp"
            android:layout_toRightOf="@id/view_line"
            android:layout_above="@id/ll_f08_location"
            android:gravity="center_vertical"
            android:orientation="horizontal">


        <ImageView
            android:id="@+id/iv_weblayout_08"
            android:layout_width="@dimen/_10sdp"
            android:layout_height="@dimen/_10sdp"
            android:src="@drawable/web_f08" />

        <TextView
            android:id="@+id/tv_websitelayout_08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/_5sdp"
            android:text="www.your website.com"
            android:textColor="@color/white"
            android:textSize="7sp"
            android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_f08_location"
            android:layout_width="@dimen/_130sdp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_location_08"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="@dimen/_12sdp"
                android:src="@drawable/location_f08"
                app:tint="#FF0006" />


            <TextView
                android:id="@+id/tv_location_08"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="Surat"
                android:textColor="@color/black"
                android:textSize="@dimen/_6sdp"
                android:textStyle="bold" />

        </LinearLayout>

    </RelativeLayout>

</FrameLayout>
